<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item  >
        <shape android:shape="oval" >
            <solid android:color="@color/line"/>
            <size
                android:width="25dp"
                android:height="25dp"/>
        </shape>
    </item>
    <item  android:bottom="1px" android:right="1px" android:left="1px" android:top="1px">
        <shape android:shape="oval" >
            <solid android:color="@color/white"/>
            <size
                android:width="25dp"
                android:height="25dp"/>
        </shape>
    </item>
</layer-list>